<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mainApplication="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <include layout="@layout/common_navigationbar"
        android:id="@+id/id_repair_navi" />
    <ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
    <ImageView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:id="@+id/topImage"
        />
    <ImageView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:id="@+id/topImage_view"
        android:scaleType="centerCrop"
        android:background="@color/mdtp_date_picker_month_day_dark_theme"
        />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:id="@+id/contact_edit_bg_name">

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/top1"
            android:layout_weight="1"
            android:text="今天"
            android:gravity="center"

            android:textSize="13dp"
            android:textColor="@color/material_blue"
            android:background="@color/white"/>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/top2"
            android:layout_weight="1"
            android:gravity="center"
            android:text="昨天"
            android:textSize="13dp"
            android:textColor="@color/mdtp_date_picker_text_disabled"
            android:background="@color/white"/>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/top3"
            android:layout_weight="1"
            android:gravity="center"
            android:text="近7天"
            android:textSize="13dp"
            android:textColor="@color/mdtp_date_picker_text_disabled"
            android:background="@color/white"/>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/top4"
            android:layout_weight="1"
            android:gravity="center"
            android:text="近30天"
            android:textSize="13dp"
            android:textColor="@color/mdtp_date_picker_text_disabled"
            android:background="@color/white"/>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/top5"
            android:layout_weight="1"
            android:gravity="center"
            android:text="自定义"
            android:textSize="13dp"
            android:textColor="@color/mdtp_date_picker_text_disabled"
            android:background="@color/white"/>



    </LinearLayout>


    <WebView
        android:layout_width="match_parent"
        android:layout_height="150dp"

        android:id="@+id/webview"
        />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:scaleType="centerCrop"
        android:background="@color/material_light_blue"
        />
    <com.points.autorepar.base.MyGridView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/homemenu_gridview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:numColumns="auto_fit"
        android:columnWidth="90dp"
        android:stretchMode="columnWidth"
        android:gravity="center"
        />
            <View
                android:layout_width="match_parent"
                android:layout_height="100dp"/>
</LinearLayout>
    </ScrollView>
</LinearLayout>
