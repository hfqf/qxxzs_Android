<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.points.autorepar.activity.contact.ContactAddNewActivity">
    <include layout="@layout/common_navigationbar"
        android:id="@+id/contact_adduser_navi" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <ImageButton
                android:id="@+id/contact_add_headurl"
                android:layout_marginTop="20dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:scaleType="fitCenter"
                android:src="@drawable/appicon"
                android:background="@color/colorWhite"
                android:layout_gravity="center_horizontal"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_gravity="center_horizontal"
                android:gravity="center_horizontal"
                android:text="点击修改头像"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="车主名:"
                    android:id="@+id/textView1"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:textSize="14dp"
                    android:textColor="#2e2a2a"
                     />
                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_name"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="请输入车主名" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="车牌号:"
                    android:id="@+id/textView2"
                    android:layout_marginTop="10dp"
                    android:textSize="14dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:id="@+id/contact_add_carcode"
                    android:background="@drawable/contactborder"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:textSize="14dp"
                    android:hint="请输入车牌号" />
                    <ImageView
                        android:textColor="@color/colorBlack"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_weight="2"
                        android:src="@drawable/car_v"
                        android:layout_marginTop="10dp"
                        android:id="@+id/car_v"
                        android:layout_marginRight="10dp"
                        android:textSize="14dp"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="车主号码:"
                    android:id="@+id/textView3"
                    android:layout_marginTop="10dp"
                    android:textSize="14dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp" />
                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_tel"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/contactborder"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="请输入车主号码" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="车型:"
                    android:id="@+id/textView4"
                    android:layout_marginTop="10dp"
                    android:textSize="14dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:id="@+id/contact_add_cartype"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="请输入车型" />

                <Button
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_weight="2"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="10dp"
                    android:id="@+id/contact_cartype_show"
                    android:layout_marginRight="10dp"

                    android:visibility="gone"
                    android:textSize="14dp"
                    android:text="车型介绍"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="车架号:"
                    android:id="@+id/textView5"
                    android:layout_marginTop="10dp"
                    android:textSize="14dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:id="@+id/contact_add_vin"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="可暂不填,待以后完善" />
                <ImageView
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_weight="2"
                    android:src="@drawable/carz_v"
                    android:layout_marginTop="10dp"
                    android:id="@+id/carz_v"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                   />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="车辆注册时间:"
                    android:id="@+id/textView6"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:textSize="12dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>

                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_carregistertime"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="0dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="年审到期前将推送提醒消息" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="下次年审时间:"
                    android:id="@+id/textView7"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:textSize="12dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>

                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_nextchecktime"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="0dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="车辆下次年审时间" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="提前提醒(天):"
                    android:id="@+id/textView8"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:textSize="12dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>

                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_tqtime1"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="0dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="提前提醒天数" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="保险公司:"
                    android:id="@+id/textView9"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:textSize="12dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>

                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_safecompany"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="0dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="保险公司" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="下次保险时间:"
                    android:id="@+id/textView10"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:textSize="12dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>

                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_nextsafetime"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="0dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="下次保险时间" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="保险提前提醒:"
                    android:id="@+id/textView11"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:textSize="12dp"
                    android:textColor="#2e2a2a"
                    android:layout_marginLeft="10dp"/>

                <EditText
                    android:textColor="@color/colorBlack"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/contact_add_tqtime2"
                    android:background="@drawable/contactborder"
                    android:layout_marginTop="0dp"
                    android:layout_marginRight="10dp"
                    android:textSize="14dp"
                    android:hint="保险提前提醒天数设置" />
            </LinearLayout>


        </LinearLayout>
    </ScrollView>
</LinearLayout>